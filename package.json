{
  "name": "drawwithcode.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "open http://localhost:3000/ & next dev",
    "build": "next build",
    "export": "next build && next export && touch out/.nojekyll",
    "pre-deploy": "rm -rf node_modules/.cache && npm run export && touch out/.nojekyll && rm -rf docs && mv out docs",
    "deploy": "npm run pre-deploy && git add docs/ && git commit -m \"Deploy website\" && git push",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fontsource/public-sans": "^4.5.11",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "gray-matter": "^4.0.3",
    "holderjs": "^2.9.9",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-dom": "18.1.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.3",
    "react-p5-wrapper": "^3.3.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "sass": "^1.51.0"
  },
  "devDependencies": {
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6"
  }
}
